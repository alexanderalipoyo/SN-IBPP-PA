{
  "name": "cypressautomation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:run_local": "node_modules/.bin/cypress open",
    "cy:run_browsers_headless": "cypress run --browser firefox & cypress run --browser edge & cypress run --browser chrome",
    "cy:run_record_with_dasboard-old-for-dev": "cypress run --record --key af646ffd-7ad7-4df9-9a4a-449d4b00d70a",
    "cy:run_record_with_dasboard-old-for-prod": "cypress run --record --key b4deb934-801d-4bb8-9da2-442e46f3736f",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox"
  },
  "author": "Alex Alipoyo",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^8.5.0",
    "@angular-devkit/build-angular": "^12.2.8",
    "@angular/cli": "^12.2.8",
    "@angular/compiler-cli": "^12.2.8",
    "@angular/language-service": "^12.2.8",
    "@nuintun/qrcode": "^3.0.1",
    "@okta/okta-auth-js": "^5.5.0",
    "angular": "^1.8.2",
    "cypress-file-upload": "^5.0.8",
    "cypress-localstorage-commands": "^1.5.0",
    "cypress-otp": "^1.0.3",
    "cypress-real-events": "^1.5.1",
    "cypress-wait-until": "^1.7.1",
    "cypress-xpath": "^1.6.2",
    "faker": "^5.5.3"
  },
  "dependencies": {
